{
	"name": "capex",
	"version": "1.0.0",
	"description": "CAPEX Sample application",
	"repository": "https://github.com/SAP-samples/btp-lcnc-capex",
	"engines": {
		"node": "^20"
	},
	"dependencies": {
		"@sap/cds": "^8",
		"@sap/mdk-tools": "^1.7.3",
		"@sap/xsenv": "^5",
		"@sap/xssec": "^4",
		"axios": "^1.7.2",
		"express": "^4.19.2",
		"passport": "^0.7.0",
		"qs": "^6.10.3",
		"@cap-js/postgres": "^1"
	},
	"scripts": {
		"start": "cds-run",
		"build": "cds build --profile pg",
		"watch": "cds watch --profile pg"
	},
	"cds": {
		"requires": {
			"db": {
				"kind": "sql"
			},
			"auth": {
				"[development]": {
					"kind": "dummy"
				},
				"[production]": {
					"kind": "xsuaa"
				}
			}
		},
		"hana": {
			"deploy-format": "hdbtable"
		}
	},
	"devDependencies": {
		"@cap-js/postgres": "^1.10.0",
		"@sap/dev-cap-tools": "^1.31.0",
		"@cap-js/cds-typer": ">=0.1"
	},
	"lcap": true,
	"imports": {
		"#cds-models/*": "./@cds-models/*/index.js"
	}
}